    "n=int(input())\n",
    "road=list(map(int, input().split()))\n",
    "cities=list(map(int, input().split()))\n",
    "\n",
    "min_val=cities[0]\n",
    "sum=0\n",
    "for i in range(n-1):\n",
    "    if min_val>cities[i]:\n",
    "        min_val=cities[i]\n",
    "    sum+=min_val*road[i]\n",
    "    \n",
    "print(sum)"
   ]
