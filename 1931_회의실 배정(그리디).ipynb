   "source": [
    "n=int(input())\n",
    "time=[]\n",
    "for _ in range(n):\n",
    "    start,end=map(int, input().split())\n",
    "    time.append([start,end])\n",
    "\n",
    "time.sort(key=lambda x:x[0])\n",
    "time.sort(key=lambda x:x[1])\n",
    "\n",
    "last=0\n",
    "cnt=0\n",
    "for i in time:\n",
    "    if i[0]>=last:        \n",
    "        cnt+=1\n",
    "        last=i[1]\n",
    "print(cnt)"
   ]
