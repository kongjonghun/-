    "n=int(input())\n",
    "\n",
    "for i in range(1, n+1):\n",
    "    num=list(map(int, str(i)))\n",
    "    if n==i+sum(num):\n",
    "        print(i)\n",
    "        break\n",
    "    if i==n:\n",
    "        print(0)\n",
    "        break"
