   "source": [
    "from collections import deque\n",
    "k=int(input())\n",
    "stack=deque()\n",
    "for _ in range(k):\n",
    "    num=int(input())\n",
    "    if num!=0:\n",
    "        stack.append(num)\n",
    "    else:\n",
    "        stack.pop()\n",
    "        \n",
    "print(sum(stack))"
   ]
  }
